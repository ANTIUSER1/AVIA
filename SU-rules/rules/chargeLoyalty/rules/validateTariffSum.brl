<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>validateTariffSum</name>
  <uuid>928af27a-1c4d-44ff-af53-328854024d7c</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[if 
	 the tariff sum of passengerLoyaltyInfo is at most 0  
then 
	set the err code of passengerLoyaltyInfo to "[tariffSum] parameter is incorrect" ;  
	set the err message of passengerLoyaltyInfo to "[tariffSum] parameter must be more than 0 but received " + the tariff sum of passengerLoyaltyInfo ;]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
