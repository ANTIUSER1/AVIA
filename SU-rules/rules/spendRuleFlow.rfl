		]]>&#13;<![CDATA[
		insert route;]]>&#13;<![CDATA[
		]]>&#13;<![CDATA[
		foreach (Bonus bonus in route.aflBonuses) {]]>&#13;<![CDATA[
			insert bonus;]]>&#13;<![CDATA[
		}]]>&#13;<![CDATA[
		]]>&#13;<![CDATA[
