			insert bonus;]]>&#13;<![CDATA[
		}]]>&#13;<![CDATA[
		]]>&#13;<![CDATA[
		foreach (Flight flight in route.flights) {]]>&#13;<![CDATA[
			]]>&#13;<![CDATA[
			foreach (Bonus bonus in flight.aflBonuses) {]]>&#13;<![CDATA[
				insert bonus;]]>&#13;<![CDATA[
			}]]>&#13;<![CDATA[
			]]>&#13;<![CDATA[
			foreach (Bonus bonus in flight.scyteamBonuses) {]]>&#13;<![CDATA[
				insert bonus;]]>&#13;<![CDATA[
			}]]>&#13;<![CDATA[
		]]>&#13;<![CDATA[
		}]]>&#13;<![CDATA[
	}]]>&#13;<![CDATA[
]]>&#13;<![CDATA[
} catch (Exception e) {]]>&#13;<![CDATA[
	e.printStackTrace();]]>&#13;<![CDATA[
	loader.release();]]>&#13;<![CDATA[
]]>&#13;<![CDATA[
} catch (RuntimeException e) {]]>&#13;<![CDATA[
	e.printStackTrace();]]>&#13;<![CDATA[
	loader.release();]]>&#13;<![CDATA[
}]]>&#13;<![CDATA[
]]></Actions>
