<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>validatePercent</name>
  <uuid>1c1ba081-7636-4071-9e47-876a4de8be20</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[if 
	 the percent of passengerLoyaltyInfo is -1 
then 
	set the err code of passengerLoyaltyInfo to "[accountingCode] parameter is incorrect" ;  
	set the err message of passengerLoyaltyInfo to "[accountingCode] parameter received " + the accounting code of passengerLoyaltyInfo + " not found"; ]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
